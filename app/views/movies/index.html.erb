<h1>UtOOb</h1>
<h2 class="center">Movies</h2>
<hr />
<div class="row">
  <div class="col s12 m6">
     <button class="btn" id="movie-form">Add Movie</button>
    <ul id="movie-list"> 
      <% @movie.each do |movie| %>
        <li
          class="movie-item"
          data-id= <%= movie.id %>
          movie-name= "<%= movie.title %>"
        >
          <%= link_to movie.title, movie_path() %>
        </li>
      <% end %>
    </ul>
  </div>
  <div class="col s12 m6" id="watch-list">
    <h2 id="watch">Watchlist</h2> 
    <ul id="play"></ul>
  </div>
</div>
